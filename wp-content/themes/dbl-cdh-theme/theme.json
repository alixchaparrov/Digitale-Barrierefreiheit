        "form": {
          "fieldHeight": "50px",
          "fieldBorderColor": "#dddddd",
          "fieldBorderRadius": "4px",
          "fieldPadding": "0.75rem 1rem",
          "fieldFocusBorderColor": "#008ab7",
          "labelSpacing": "0.5rem"
        },
        "newsletter": {
          "backgroundColor": "#f5f5f5",
          "borderRadius": "4px",
          "padding": "2rem"
