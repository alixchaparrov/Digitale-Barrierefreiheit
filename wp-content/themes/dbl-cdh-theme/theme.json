{
    "$schema": "https://schemas.wp.org/wp/6.5/theme.json",
    "version": 2,
    "settings": {
        "color": {
            "palette": [
                {
                    "slug": "dunkel-blau",
                    "color": "#151b35",
                    "name": "Dunkel Blau"
                },
                {
                    "slug": "haupt-blau",
                    "color": "#008ab7",
                    "name": "Haupt Blau"
                },
                {
                    "slug": "akzent-rot",
                    "color": "#E50046",
                    "name": "Akzent Rot"
                },
                {
                    "slug": "hell-blau",
                    "color": "#65C8E8",
                    "name": "Hell Blau"
                },
                {
                    "slug": "hintergrund-grau",
                    "color": "#f5f5f5",
                    "name": "Hintergrund Grau"
                },
                {
                    "slug": "weiss",
                    "color": "#ffffff",
                    "name": "Weiß"
                },
                {
                    "slug": "text-dunkel",
                    "color": "#333333",
                    "name": "Text Dunkel"
                }
            ],
            "background": true,
            "text": true,
            "link": true,
            "custom": true
        },
        "typography": {
            "fontFamilies": [
                {
                    "fontFamily": "Noto Sans, sans-serif",
                    "slug": "noto-sans",
                    "name": "Noto Sans"
                }
            ],
            "fontSizes": [
                {
                    "slug": "klein",
                    "size": "0.875rem",
                    "name": "Klein"
                },
                {
                    "slug": "normal",
                    "size": "1rem",
                    "name": "Normal"
                },
                {
                    "slug": "mittel",
                    "size": "1.25rem",
                    "name": "Mittel"
                },
                {
                    "slug": "gross",
                    "size": "1.5rem",
                    "name": "Groß"
                },
                {
                    "slug": "extra-gross",
                    "size": "2rem",
                    "name": "Extra Groß"
                },
                {
                    "slug": "xxl",
                    "size": "2.5rem",
                    "name": "XXL"
                }
            ],
            "fontWeight": true,
            "lineHeight": true,
            "dropCap": true,
            "customFontSize": true
        },
        "spacing": {
            "padding": true,
            "margin": true,
            "blockGap": true,
            "units": ["rem", "em", "px", "%", "vh", "vw"],
            "spacingSizes": [
                {
                    "slug": "extra-klein",
                    "size": "0.5rem",
                    "name": "Extra Klein"
                },
                {
                    "slug": "klein",
                    "size": "1rem",
                    "name": "Klein"
                },
                {
                    "slug": "mittel",
                    "size": "1.5rem",
                    "name": "Mittel"
                },
                {
                    "slug": "gross",
                    "size": "2rem",
                    "name": "Groß"
                },
                {
                    "slug": "extra-gross",
                    "size": "3rem",
                    "name": "Extra Groß"
                }
            ]
        },
        "layout": {
            "contentSize": "1140px",
            "wideSize": "1320px"
        },
        "border": {
            "color": true,
            "radius": true,
            "style": true,
            "width": true
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--weiss)",
            "text": "var(--wp--preset--color--text-dunkel)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--noto-sans)",
            "lineHeight": "1.6"
        },
        "elements": {
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--akzent-rot)",
                    "text": "var(--wp--preset--color--weiss)"
                },
                "border": {
                    "radius": "0.25rem"
                }
            },
            "link": {
                "color": {
                    "text": "var(--wp--preset--color--haupt-blau)"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--xxl)",
                    "fontWeight": "700"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--extra-gross)",
                    "fontWeight": "700"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--gross)",
                    "fontWeight": "600"
                }
            }
        },
        "blocks": {
            "core/button": {
                "typography": {
                    "fontWeight": "600"
                }
            },
            "core/separator": {
                "color": {
                    "background": "var(--wp--preset--color--hell-blau)"
                }
            },
            "core/quote": {
                "border": {
                    "left": {
                        "color": "var(--wp--preset--color--akzent-rot)",
                        "width": "4px"
                    }
                },
                "spacing": {
                    "padding": {
                        "left": "1.5rem"
                    }
                }
            },
            "core/social-links": {
                "spacing": {
                    "margin": {
                        "top": "var(--wp--preset--spacing--mittel)"
                    }
                }
            }
        }
    },
    "templateParts": [
        {
            "name": "header",
            "title": "Header",
            "area": "header"
        },
        {
            "name": "footer",
            "title": "Footer",
            "area": "footer"
        }
    ]
}